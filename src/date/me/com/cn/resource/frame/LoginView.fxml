<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="date.me.com.cn.view.controller.LoginViewController">
   <children>
      <GridPane layoutX="200.0" layoutY="105.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label fx:id="username" text="用户名" />
            <Label fx:id="passwd" text="密码" GridPane.rowIndex="1" />
            <PasswordField fx:id="psdfield" prefHeight="30.0" prefWidth="200.0" promptText="请输入密码..." GridPane.columnIndex="1" GridPane.rowIndex="1">
               <font>
                  <Font size="11.0" />
               </font>
            </PasswordField>
            <TextField fx:id="textfield" prefHeight="30.0" prefWidth="200.0" promptText="请输入用户名..." GridPane.columnIndex="1">
               <font>
                  <Font size="11.0" />
               </font>
            </TextField>
         </children>
      </GridPane>
      <Button fx:id="login" layoutX="266.0" layoutY="176.0" mnemonicParsing="false" onAction="#loginclick" text="登陆" />
      <Label layoutX="195.0" layoutY="49.0" text="欢迎使用date-me">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
